<body>
    @using (Html.BeginForm("cajaMovimientos", "CAJA", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        var userName = Session["LoggedUser"];
        var fecha2 = DateTime.Now.ToLongDateString();
        var estado = "Caja cerrada";
        if (Session["EstadoCaja"] != null)
        {
            estado = Session["EstadoCaja"].ToString();
        }
        <form action="" method="post">
            <div class="modal fade caja" tabindex="-1" role="dialog" aria-hidden="true">
                <div class="modal-dialog modal-lg">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal">
                                <span aria-hidden="true">×</span>
                            </button>
                            <h2>@(fecha2)</h2>
                            <h1 id="myModalLabel">Apertura de caja&nbsp; </h1>
                        </div>
                        <div class="modal-body">
                            <div class="x_panel">
                                <div class="form-group">
                                    <table>
                                        <tr>
                                            <td><h2>Usuario: </h2></td>
                                            <td><input type="text" name="usuario" value="@(userName)" style="width: 340px" class="form-control" /></td>
                                            <td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>
                                            <td align="center"><h2>Monto base: $</h2></td>
                                            <td><input type="number" name="monto" class="form-control" /></td>
                                        </tr>
                                        <tr>
                                            <td><h2>Caja:</h2></td>
                                            <td><select name="caja" style="width: 340px" class="form-control"><option>1</option><option>2</option></select></td>
                                            <td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>
                                            <td align="center"><h2>Observaciones:</h2></td>
                                            <td>
                                                <input type="text" name="observaciones" style="WIDTH: 228px; HEIGHT: 98px"
                                                       size=32 />
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                            </div><br>
                            <div class="modal-footer">
                                <table>
                                    <tr>
                                        <td><h1>Estado:</h1></td>
                                        <td><h2 class="bg-red">@(estado)</h2></td>
                                        <td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>

                                        @if (ViewData["userFlag"] != null)
                                        {
                                            if ((bool)ViewData["userFlag"] == true)
                                            {
                                                <td><input type="submit" class="btn btn-primary" value="Guardar" disabled></td>
                                                <td><button type="button" class="btn btn-primary" id="cambioTurno" data-toggle="modal" data-target=".cambio_turno" data-dismiss="modal">Cambio de turnos</button></td>
                                                <td><button type="button" id="cierre" class="btn btn-primary">Realizar cierre</button></td>
                                                <td><button type="button" id="reportes" class="btn btn-primary">Reportes</button></td>
                                            }
                                        }
                                        else
                                        {
                                            <td><input type="submit" class="btn btn-primary" value="Guardar"></td>
                                            <td><button type="button" id="cambioTurno" class="btn btn-primary" disabled>Cambio de turno</button></td>
                                            <td><button type="button" id="cierre" class="btn btn-primary" disabled>Realizar cierre</button></td>
                                            <td><button type="button" id="reportes" class="btn btn-primary" disabled>Reportes</button></td>
                                        }

                                    </tr>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </form>

    }

