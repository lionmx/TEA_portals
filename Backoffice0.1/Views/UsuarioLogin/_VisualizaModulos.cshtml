@model IEnumerable<Backoffice0._1.Models.C_modulos_sub_permisos>

@foreach (var item in Session["modulos"] as List<Backoffice0._1.Models.ModulosUsuario>)
{
    <li>
        <a><i class="@item.Icono"></i>@item.Nombre_modulo <span class="fa fa-chevron-down"></span></a>
        <ul class="nav child_menu" style="display: none;">
            @foreach (var item2 in Session["submodulos"] as List<Backoffice0._1.Models.SubmodulosUsuario>)
            {
                if (item2.Id_modulo == item.Id_modulo)
                {
                    <li>
                        @Html.ActionLink(item2.Nombre_submodulo, item2.Funcion, item2.Controlador)
                    </li>
                }
            }
        </ul>
    </li>
}

