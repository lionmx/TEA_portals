<table class="table table-striped ">
    <thead>
        <tr class="headings">
            <th class="column-title">#</th>
            <th class="column-title">Producto </th>
            <th class="column-title">Costo </th>
            <th class="bulk-actions" colspan="7">
                <a class="antoo" style="color:#fff; font-weight:500;">Bulk Actions ( <span class="action-cnt"> </span> ) <i class="fa fa-chevron-down"></i></a>
            </th>
        </tr>
    </thead>
    <tbody>
        @if (Session["CarritoCombo"] != null)
        {
            foreach (var item in Session["CarritoCombo"] as List<Backoffice0._1.Models.CarritoComboItem>)
            {
                <tr class="even pointer">
                    <td class=" ">1</td>
                    <td class=" ">@item.Producto </td>
                    <td class=" ">@item.Costo</td>
                    <!-- <td>  <a href="#" onclick="" class="btn btn-danger btn-xs"><i class="fa fa-remove"></i></a></td> -->
                </tr>
            }
        }
    </tbody>
</table>


