@model IEnumerable<Backoffice0._1.Models.C_cajas>

@{
    ViewBag.Title = "";
    Layout = "~/Views/Shared/MainLayout.cshtml";
}



<p>
    @Html.ActionLink("Crear nueva", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.caja)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.status)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.C_sucursales.nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.caja)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.C_sucursales.nombre)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.id_caja }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.id_caja }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.id_caja })
        </td>
    </tr>
}

</table>
